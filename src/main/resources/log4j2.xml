<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error" name="ConsoleExample" packages="simplesolution.dev">
<Appenders>
    <Console name="STDOUT">
        <PatternLayout pattern="%d - %t - %p - %c - %m%n"/>
    </Console>
</Appenders>
<Loggers>
    <Root level="trace">
        <AppenderRef ref="STDOUT" />
    </Root>
</Loggers>
</Configuration>